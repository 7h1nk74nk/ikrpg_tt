					</size>
				</anchored>
				<icon>indicator_expanddown</icon>
				<script>
					function onValueChanged()   
                        window.prerequisite.setVisible(getValue());
                        window.description.setVisible(getValue());	                        
					end
				</script>
			</button_toggle>
           
        </sheetdata>
