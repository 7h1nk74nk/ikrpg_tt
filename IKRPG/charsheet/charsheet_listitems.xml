			<stringfield name="description">
				<anchored>
                    <to>label</to>
                    <position>belowleft</position>
                    <offset>0,5</offset>		
					<size>						
						<width>320</width>
					</size>
				</anchored>
				<font>sheettextsmall</font>     				
				<lineoffset default="on">-1</lineoffset>
				<multilinespacing>18</multilinespacing>  
				<invisible/>
			</stringfield>
			
		</sheetdata>
	</windowclass>

