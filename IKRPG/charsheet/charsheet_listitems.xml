	<windowclass name="charsheet_mechanikaitem">
		<sizelimits>
			<minimum>
				<height>10</height>
			</minimum>
		</sizelimits>
		<sheetdata>
		
			<link_smart name="shortcut">
				<bounds>-18,0,20,20</bounds>
				<defaultclass>ref_ability</defaultclass>
				<toggle />
			</link_smart>

			<string_textlistitem name="value">
				<anchored>
					<top />
					<left><offset>0</offset></left>
					<right><offset>-200</offset></right>
				</anchored>
				<multilinespacing>20</multilinespacing>
			</string_textlistitem>
			
			
			
			<string_textlistitem name="effect">
				<anchored>
					<to>value</to>
					<position>right</position>
					<offset>10,0</offset>
					<size>
						<width>160</width>
						<height>14</height>
					</size>
				</anchored>
				<multilinespacing>20</multilinespacing>
			</string_textlistitem>
			
		</sheetdata>
	</windowclass>

	
