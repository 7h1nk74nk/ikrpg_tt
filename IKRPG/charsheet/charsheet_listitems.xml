	<!-- SKILL LIST ITEM -->	

	<windowclass name="charsheet_skill">
		<sizelimits>
			<minimum>
				<height>20</height>
			</minimum>
		</sizelimits>  
        <script file="charsheet/scripts/charsheet_skill.lua" />
		<sheetdata>
            
            <numberfield name="order">
                <bounds>0,0,0,0</bounds>
                <invisible/>
            </numberfield>
            
			
            <numberfield name="subskillnumber">
                <bounds>0,0,0,0</bounds>
                <invisible/>
            </numberfield>

            <stringfield name="parentskill">
                <bounds>0,0,0,0</bounds>
                <invisible/>
            </stringfield>                     
            
			<stringfield name="label">
				<anchored>
					<top><offset>5</offset></top>
					<left><offset>0</offset></left>					
					<size>
						<height>18</height>
                        <width>130</width>
					</size>
				</anchored>
                <font>sheettext</font>                                                                  
                <lineoffset default="on">-1</lineoffset>
			</stringfield>
            
            <button_stringcycler name="skilltype">
				<anchored>
					<to>label</to>
                    <position>righthigh</position>
                    <offset>40,-2</offset>
					<size>
						<height>18</height>
						<width>32</width>
					</size>
				</anchored>
				<font>sheettextsmall</font>
				<center />
				<frame>
					<name>bonus</name>
					<offset>6,5,6,5</offset>
				</frame>
				<parameters>
					<labels>Mil.|Gen.|Occ.</labels>					
                    <values>Mil.|Gen.|Occ.</values>
					<defaultlabel> - </defaultlabel>
				</parameters>
			</button_stringcycler>
            
            <button_stringcycler name="skillstat">
				<anchored>
					<to>skilltype</to>
                    <position>righthigh</position>
                    <offset>5,0</offset>
					<size>
						<height>18</height>
						<width>32</width>
					</size>
				</anchored>
				<font>sheettextsmall</font>
				<center />
				<frame>
					<name>bonus</name>
					<offset>6,5,6,5</offset>
				</frame>
				<parameters>
					<labels>PHY|SPD|STR|AGI|PRW|POI|INT|ARC|PER</labels>					
                    <values>PHY|SPD|STR|AGI|PRW|POI|INT|ARC|PER</values>
					<defaultlabel>SOC</defaultlabel>
				</parameters>
			</button_stringcycler>
            
            
            
			<basicnumber name="max">
                <anchored>
                    <to>skillstat</to>
                    <position>righthigh</position>
                    <offset>5,0</offset>
                    <size>
                        <width>32</width>
                        <height>18</height>
                    </size>
                </anchored>
                <min>0</min>
                <max>4</max>
                
            </basicnumber>
            
            <basicnumber name="rank">
                <anchored>
                    <to>skillstat</to>
                    <position>righthigh</position>
                    <offset>42,0</offset>
                    <size>
                        <width>32</width>
                        <height>18</height>
                    </size>
                </anchored>
                
            </basicnumber>
            
            <number_skill name="total">
                <anchored>
                    <to>rank</to>
                    <position>righthigh</position>
                    <offset>5,0</offset>
                    <size>
                        <width>32</width>
                        <height>18</height>
                    </size>
                </anchored>
				<namesource>label</namesource>
                <readonly/>	
            </number_skill>		
			
		</sheetdata>
	</windowclass>
    
