<root>

	<!-- WEAPON LIST ITEM -->

	<windowclass name="charsheet_weapon">
		<sizelimits>
			<minimum>
				<height>45</height>
			</minimum>
		</sizelimits>          
		<sheetdata>
                        
            <button_iconcycler name="type">
                <anchored>
					<top><offset>2</offset></top>
					<left><offset>2</offset></left>					
					<size>
						<height>40</height>
                        <width>40</width>
					</size>
				</anchored>                
                <stateframe>
                    <hover>
                        <name>sheetfocus</name>
                        <offset>6,5,6,5</offset>
                        <hidereadonly />
                    </hover>
                </stateframe>
                <parameters>
                    <icons>action_melee</icons>
                    <values>melee</values>   
                    <defaulticon>action_ranged</defaulticon>                    
                </parameters>
            </button_iconcycler>
                
			<stringfield name="weaponlabel">
				<anchored>
					<to>type</to>
                    <position>righthigh</position>
                    <offset>5,0</offset>					
					<size>
						<height>18</height>
                        <width>150</width>
					</size>
				</anchored>
                <font>sheettext</font>                                                                  
                <lineoffset default="on">-1</lineoffset>
			</stringfield>
            
            
            <stringfield name="notes">
				<anchored>
                    <to>weaponlabel</to>
                    <position>belowleft</position>
                    <offset>0,0</offset>
                    <size>
						<height>18</height>
                        <width>190</width>
					</size>
                </anchored>
                <font>sheettext</font>                                                                  
                <lineoffset default="on">-1</lineoffset>
			</stringfield>
			
			<stringfield name="ammo">
				<anchored>
                    <to>notes</to>
                    <position>right</position>
                    <offset>10,0</offset>
                    <size>
						<height>18</height>
                        <width>115</width>
					</size>
                </anchored>
                <font>sheettext</font>                                                                  
                <lineoffset default="on">-1</lineoffset>
			</stringfield>
			
            <basicnumber name="range">
                <anchored>
                    <to>weaponlabel</to>
                    <position>righthigh</position>
                    <offset>10,0</offset>
                    <size>
                        <width>32</width>
                        <height>18</height>
                    </size>
                </anchored>
            </basicnumber>
            
			<number_RAT name="rat">
                <anchored>
                    <to>range</to>
                    <position>righthigh</position>
                    <offset>10,0</offset>
                    <size>
                        <width>32</width>
                        <height>18</height>
                    </size>
                </anchored>
				<namesource>weaponlabel</namesource>
            </number_RAT>
            
            <number_MAT name="mat">
                <anchored>
                    <to>rat</to>
                    <position>righthigh</position>
                    <offset>10,0</offset>
                    <size>
                        <width>32</width>
                        <height>18</height>
                    </size>
                </anchored>  
				<namesource>weaponlabel</namesource>				
            </number_MAT>
            
            <number_DMG name="damage">
                <anchored>
                    <to>mat</to>
                    <position>righthigh</position>
                    <offset>10,0</offset>
                    <size>
                        <width>32</width>
                        <height>18</height>
                    </size>
                </anchored>  
				<namesource>weaponlabel</namesource>	
            </number_DMG>
			
		</sheetdata>
	</windowclass>
	
